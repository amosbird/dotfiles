rofi.opacity: 80
rofi.kb-cancel: Escape,Control+bracketleft
rofi.kb-row-up: Control+k
rofi.kb-row-down: Control+j
rofi.kb-accept-entry: Control+l,Return,KP_Enter
rofi.parse-hosts: true
