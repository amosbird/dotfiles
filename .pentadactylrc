loadplugins '\.(js|penta)$'

group user
set hintkeys=asdflkj
set editor="em"
set guioptions=''
set showtabline=never
map -modes=n s tg<Space>
map -count -modes=c,i,n,t,v <C-c> <Pass>
map -count -modes=n <C-a> <Pass>
map -count -modes=n = <count>+
map -count -modes=n gb <count>:bmarks!<Space>
map -count -modes=n gh <count>:history!<Space>
map -count -modes=n h <count>H
map -count -modes=n l <count>L
map -count -modes=n <C-,> <count>gT
map -count -modes=n <C-.> <count>gt
map -count -modes=c <C-f> <Right>
map -count -modes=c,i,t <C-v> <Pass>
map -count -modes=c,i,t <C-x> <Pass>
map -count -modes=c,i,t <C-z> <Pass>
hi FontFixed -a font: 16px "Microsoft YaHei Mono" !important;
set nextpattern='\s*下页\s*','\s*下一页\s*','\bnext\b',^>$,'^(>>|»)$','^(>|»)','(>|»)$','\bmore,'
set previouspattern='\s*上页\s*','\s*上一页\s*','\bprev|previous\b',^<$,'^(<<|«)$','^(<|«)','(<|«)$,'
set complete=l
hi Hint font-family: consolas; font-size: 16px; font-weight: bold; color: black; background-color: red; border-color: ButtonShadow; border-width: 1px; border-style: solid;
